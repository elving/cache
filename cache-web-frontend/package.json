{
  "name": "cache-web-frontend",
  "version": "1.14.0",
  "description": "Cache. Collect Everything",
  "main": "src/index.js",
  "scripts": {
    "deploy": "gulp && npm run updateSHA && npm run pushOrigin",
    "development": "NODE_ENV=development ./node_modules/.bin/babel-node server.js",
    "production": "NODE_ENV=production webpack --progress --colors --config webpack.production.config.babel.js",
    "pushOrigin": "git push origin master && git push --tags origin",
    "updateSHA": "hash=$(git rev-parse HEAD);ssh root@104.131.32.94 dokku config:set app.trycache.co GIT_SHA=$hash",
    "cleanInstall": "npm cache clear && rm -rf node_modules && npm i"
  },
  "author": "Elving Rodriguez",
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.9.1",
    "dotenv": "^1.2.0",
    "eslint": "^0.19.0",
    "eslint-loader": "^0.10.0",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^0.3.8",
    "gulp": "^3.9.0",
    "gulp-gzip": "^1.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-s3": "^0.3.0",
    "react-hot-loader": "^1.2.8",
    "run-sequence": "^1.1.2",
    "style-loader": "^0.8.3",
    "stylus-loader": "^1.1.1",
    "webpack": "^1.5.1",
    "webpack-dev-server": "^1.7.0",
    "webpack-stream": "^2.1.0"
  },
  "dependencies": {
    "axios": "^0.5.4",
    "babel": "^5.0.1",
    "babel-core": "^5.0.2",
    "babel-runtime": "^5.0.2",
    "blueimp-md5": "^1.1.1",
    "classnames": "^1.2.0",
    "flummox": "4.0.0-alpha",
    "jsonp": "^0.2.0",
    "lodash": "^3.8.0",
    "moment": "^2.10.3",
    "normalize.css": "^3.0.2",
    "react": "^0.13.3",
    "react-router": "0.13.3",
    "react-select": "^0.6.11",
    "react-social": "^0.1.5",
