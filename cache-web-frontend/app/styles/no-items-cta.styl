.no-items-cta {
  width: 700px;
  height: 70vh;
  display: flex;
  align-items: center;
  margin-left: auto;
  margin-right: auto;
  flex-direction: column;
  justify-content: center;
}

.no-items-cta__text,
.no-items-cta__title {
  font-size: 48px;
  text-align: center;
  font-family: $font;
  font-weight: 400;
}

.no-items-cta__text {
  font-size: 24px;
}

